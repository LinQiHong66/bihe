<div class="default">
			<h2 class="sa_tit" ng-bind="sTit"></h2>
			<!-- 安全等级信息 begin -->
			<div class="level clearfix">
				<ul>
					<li class="sa_sl">
						<div class="sl_top clearfix">
							<span ng-bind="lev0"></span>
							<span class="sl_jdt">
								<i>
									<b ng-style='lev1'></b>
								</i>
							</span>
							<span class="sl_level" ng-bind='lev2'></span>
						</div>
						<div class="sl_bot" ng-bind='lev3'>
							完成填写常用邮箱，将极大提升您的账户安全性
						</div>
					</li>
					<li class='sa_info'>
						<span ng-bind='sId'></span>
						<span ng-bind='sName'></span>
						<span ng-bind='sUser'></span>
						<span>{{mailphone}}{{sEmail}}</span>
					</li>
				</ul>
			</div>
			<!-- 安全等级信息 end -->
