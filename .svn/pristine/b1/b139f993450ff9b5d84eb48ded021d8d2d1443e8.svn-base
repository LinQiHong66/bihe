@charset "UTF-8";
//这里的嵌套格式一定要用花阔号，
//每句都要用结束符可以使用私有属性
@import "_config.scss";
@import "_mixins.scss";
@import "variable.scss";
.liftNow{
    width: 96%;
    padding: 20px 2%;
    h3{
        padding: 22px;
        font-size: 22px;
    }
     select,input{
            border: 1px solid #ccc;
        }
    .liftNowT{
        padding: 10px;
        .liftNowTT{
            label{
                width: 30%;
                display: inline-block;
                text-align: right;
            }
            select{
                height: 30px;
                width: 200px;
            }
            .fl{
                width: 50%;
                line-height: 30px;
            }
        }
       .liftNowTB{
           .liftNowTBL,.liftNowTBR{
               width: 50%;
               text-align: left;
           }
           .liftNowTBL{
               label{
                    width: 30%;
                    display: inline-block;
                    text-align: right;
               }
                >div{
                   margin: 10px 0px;
                }
               input{
                   padding: 0;
                   padding-left: 5px;
                   width: 193px;
                   height: 26px;
               }
               span:nth-child(3),a{
                   padding-left: 10px;
               }
           }
           .liftNowTBR{
               >div{
                   margin: 10px 0px;
               }
               input{
                   padding: 0;
                   padding-left: 5px;
                   width: 101px;
                   height: 26px;
               }
               span:nth-child(2){
                   background-color: $tcolor1;
                   height: 28px;
                   line-height: 28px;
                   cursor: pointer;
                   padding: 0 10px;
                   &:hover{
                       background-color: $tcolor2;
                   }
               }
               span:nth-child(3){
                    line-height: 30px;
                    margin-left: 5px;
               }
               .RBtn{
                   span{
                       display: inline-block;
                        width: 200px;
                        background-color: $tcolor1;
                        height: 30px;
                        line-height: 30px;
                        color: #fff;
                        text-align: center;
                        margin-left: 70px;
                        cursor: pointer;
                        &:hover{
                            background-color: $tcolor2;
                        }
                   }
               }
           }
       }
    } 
    .point{
        background-color:$pointbg;
        h5{
            padding: 10px;
        }
        p{
            padding-left: 20px;
            padding-bottom: 10px;
        }
    } 
    .record{
        h4{
            padding: 10px;
        }
        ul{
             border-bottom: 1px solid rgba(204, 204, 204, 0.4);
            li{
                text-align: center;
                padding: 10px 0;
                float: left;
                width: $width/6;
            }
        }
        ul.ulHeader {
            background-color: $ulcolor;
        }
        ul.ulData{li{font-size:14px}}
        .Jpage{
        text-align: center;
        span{
            background-color: $tcolor1;
            padding: 5px 10px;
            cursor: pointer;
            margin:0 10px; 
            color: #fff;
            &:hover{
                background-color: $tcolor2;
            }
        }
        p:nth-child(1){
            padding-top: 20px;
        }
        p{
            padding-top:15px;
        }
        p select{
            width: 50px;
            margin: 0 10px;
        }
    }
    }        

}
