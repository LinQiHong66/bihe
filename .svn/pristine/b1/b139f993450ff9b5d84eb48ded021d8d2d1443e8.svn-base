//最大宽屏992px的时候
@include respond-to(md){
    .liftNow{
        .liftNowT .liftNowTT {
         .fl {
            width: 90%;
            line-height: 30px;
           }
           label { width: 15%;}
        }
    }
}

//最大宽屏768px的时候
.liftNow{
   
    .liftNowT .liftNowTB .liftNowTBL{
        @include respond-to(sm){
            float: none;
            width: 100%;
        }
        >div{
            @include respond-to(sm){
                width: 100%;
            } 
            label{
                @include respond-to(sm){
                 width: 15%;
                }  
            }
        }
    }
    .liftNowT .liftNowTB  .liftNowTBR{
        @include respond-to(sm){
            float: none;
            width: 100%;
        }
        .clearfix>div{
            @include respond-to(sm){
                margin-left: 5.2%;
            }  
        }
        .RBtn{
            @include respond-to(sm){
                margin-left: 5.2%;
            }
        }
    }
}
//最大宽屏480px的时候
.liftNow{
    .record{
        @include respond-to(xs){
            overflow: scroll;
        }
        ul{
           @include respond-to(xs){
                min-width: 780px;
            }
        }
    }
    .liftNowT{
        .liftNowTT .fl{
            @include respond-to(xs){
                float: none;
                width: 100%;
            }
            label{
                @include respond-to(xs){
                    display:block;
                    width: auto;
                    text-align: left;
                }
            }
        }
        .liftNowTB .liftNowTBL > div label{
             @include respond-to(xs){
                    display:block;
                    width: auto;
                    text-align: left;
                }   
        }
        .liftNowTB .liftNowTBR .clearfix > div{
            @include respond-to(xs){
                   margin-left: 0;
                   width: 100%;
                   margin-bottom: 5px;
                }
            span{
                @include respond-to(xs){
                    display: block;
                    width: auto;
                    text-align: left;
                    margin-bottom: 1px;
                }
            } 
            input{
                @include respond-to(xs){
                    width: 191px;
                }
            }    
        }
        .liftNowTB .liftNowTBR .RBtn{
             @include respond-to(xs){
                   margin-left: 0;
            }
            span{
                @include respond-to(xs){
                   margin-left: 0;
                }
            }
        }
    }
    .record .Jpage{
        p{
            @include respond-to(xs){
                min-width: 680px;
            } 
        }
    }
}
